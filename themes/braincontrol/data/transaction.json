{
    "dashboard": {
        "panels": [
            {
                "id": "welcome-message",
                "css": "col-md-12 ribbon",
                "body": {
                    "func": "bootstrap",
                    "type": "jumbotrons",
                    "objects": [
                        {
                            "title": "Transaction ID: {{vars.txid}}",
                            "intro": "This information about the transaction comes from the device, not the blockchain",
                            "buttons": [
                                {
                                    "href": "{{urls.tx}}",
                                    "css": "btn-success {{tx.hidden}}",
                                    "text": "LIVE DATA"
                                }
                            ]
                        }
                    ]
                }
            },
            {
                "id": "tx-data",
                "css": "col-md-6 odd",
                "header": "Transaction Data",
                "body": {
                    "func": "bootstrap",
                    "type": "lists",
                    "objects": [
                        {
                            "items": [
                                {
                                    "html": "Size",
                                    "buttons": [
                                        {
                                            "css": "btn-xs disabled pull-right",
                                            "text": "{{tx.size}}"
                                        }
                                    ]
                                },
                                {
                                    "html": "Time",
                                    "buttons": [
                                        {
                                            "css": "btn-xs disabled pull-right",
                                            "text": "{{tx.time}}"
                                        }
                                    ]
                                },
                                {
                                    "html": "Block",
                                    "buttons": [
                                        {
                                            "css": "btn-xs disabled pull-right",
                                            "text": "{{tx.block}}"
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            },
            {
                "id": "tx-amounts",
                "css": "col-md-6 even",
                "header": "Transaction Amounts",
                "body": {
                    "func": "bootstrap",
                    "type": "lists",
                    "objects": [
                        {
                            "items": [
                                {
                                    "html": "Total Input",
                                    "buttons": [
                                        {
                                            "css": "btn-xs disabled pull-right",
                                            "text": "{{tx.input}}"
                                        }
                                    ]
                                },
                                {
                                    "html": "Total Output",
                                    "buttons": [
                                        {
                                            "css": "btn-xs disabled pull-right",
                                            "text": "{{tx.output}}"
                                        }
                                    ]
                                },
                                {
                                    "html": "Fees",
                                    "buttons": [
                                        {
                                            "css": "btn-xs disabled pull-right",
                                            "text": "{{tx.fees}}"
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            }
        ]
    }
}