{
    "user": {
        "setup": {
            "func": "got",
            "collection": "blockstrap",
            "key": "salt"
        },
        "name": {
            "func": "get",
            "collection": "keys",
            "key": "your_name"
        },
        "avatar": {
            "func": "avatars",
            "default": "blockstrap/img/avatar.jpg"
        },
        "last": {
            "func": "last",
            "html": "last transaction {{ago}}",
            "type": "tx",
            "alternative": "no transactions yet"
        },
        "urls": {
            "profile": "#profile"
        }
    },
    "navigation": [
        {
            "id": "dashboard",
            "css": "btn-page active",
            "href": "#dashboard",
            "text": "Dashboard"
        },
        {
            "id": "accounts",
            "css": "btn-page",
            "href": "#accounts",
            "text": "Accounts"
        },
        {
            "id": "contacts",
            "css": "btn-page",
            "href": "#contacts",
            "text": "Contacts"
        },
        {
            "id": "send",
            "css": "btn-page",
            "href": "#send",
            "text": "Send"
        },
        {
            "id": "docs",
            "css": "",
            "href": "https://github.com/blockstrap/framework/tree/master/docs/en",
            "text": "Help"
        }
    ],
    "dashboard": {
        "panels": [
            {
                "id": "welcome-message",
                "css": "col-md-12 ribbon",
                "body": {
                    "func": "bootstrap",
                    "type": "jumbotrons",
                    "objects": [
                        {
                            "title": "Welcome back {{user.name}}",
                            "intro": "Here is an overview of your recent accounts and the bitcoin markets.",
                            "html": "You are currently communicating with the blockchains via {{user.api}} and have <a href='#accounts' class='btn-page'>{{user.accounts}}</a> and <a href='#contacts' class='btn-page'>{{user.contacts}}</a> stored on this device",
                            "buttons": [
                                {
                                    "href": "#",
                                    "css": "btn-danger btn-reset",
                                    "text": "Reset Device"
                                },
                                {
                                    "href": "#",
                                    "css": "btn-default",
                                    "text": "Backup",
                                    "attributes": [
                                        {
                                            "key": "data-toggle",
                                            "value": "modal"
                                        },
                                        {
                                            "key": "data-target",
                                            "value": "#backup-modal"
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            },
            {
                "id": "recent-transactions",
                "css": "col-md-6 odd",
                "header": "Recent Transactions",
                "body": {
                    "func": "txs"
                }
            },
            {
                "id": "market-conditions",
                "css": "col-md-6 even",
                "header": "BTC > USD - Market Conditions <a href='#' class='pull-right btn btn-sm btn-default btn-markets'>Refresh</a>",
                "body": {
                    "func": "plugin",
                    "name": "markets",
                    "call": "filter",
                    "data": [
                        {
                            "css": "col-sm-6 panel",
                            "id": "price_usd_now"
                        },
                        {
                            "css": "col-sm-6 panel",
                            "id": "tx_count_24hr"
                        },
                        {
                            "css": "col-sm-6 panel",
                            "id": "sent_usd_24hr"
                        },
                        {
                            "css": "col-sm-6 panel",
                            "id": "sent_coins_24hr"
                        },
                        {
                            "css": "col-sm-6 panel",
                            "id": "coins_discovered"
                        },
                        {
                            "css": "col-sm-6 panel",
                            "id": "marketcap"
                        }
                    ]
                }
            }
        ]
    },
    "form": {
        "search": {
            "id": "search-term",
            "css": "icon fa fa-search btn btn-xs",
            "placeholder": "Search your wallet"
        }
    },
    "footer": "powered by <a href='http://blockstrap.com'>blockstrap.com</a>",
    "modals": {
        "func": "bootstrap",
        "type": "modals",
        "objects": [
            {
                "id": "default",
                "title": "Title",
                "body": "<p>Content.</p>",
                "close": "Cancel"
            },
            {
                "id": "confirm",
                "title": "Confirmation Required",
                "body": "<p>Please re-confirm you want to do that?</p>",
                "close": "Cancel",
                "footer": true,
                "actions": [
                    {
                        "text": "Confirm",
                        "css": "btn-success"
                    }
                ] 
            },
            {
                "id": "login-credentials",
                "title": "Create Login Credentials",
                "body": "<p>Add an extra layer of optional protection in case you share your computer and phone with others. This is far from secure and easily fooled and has nothing to do with the storing of your coins, only the protection from others using this app without your permission.</p>",
                "form": {
                    "css": "form-horizontal",
                    "fields": [
                        {
                            "inputs": {
                                "id": "device_username",
                                "label": {
                                    "text": "Username",
                                    "css": "col-xs-3"
                                },
                                "type": "text",
                                "placeholder": "This can be anything you like and is not registered anywhere",
                                "wrapper": {
                                    "css": "col-xs-9"
                                }
                            }
                        },
                        {
                            "inputs": {
                                "id": "device_password",
                                "label": {
                                    "text": "Password",
                                    "css": "col-xs-3"
                                },
                                "type": "password",
                                "placeholder": "Something simple - this is not for storing coins",
                                "wrapper": {
                                    "css": "col-xs-9"
                                }
                            }
                        },
                        {
                            "inputs": {
                                "id": "device_password_repeat",
                                "css": "ignore",
                                "label": {
                                    "text": "Repeat",
                                    "css": "col-xs-3"
                                },
                                "type": "password",
                                "placeholder": "Better to be safe than sorry",
                                "wrapper": {
                                    "css": "col-xs-9"
                                }
                            }
                        }
                    ],
                    "buttons": {
                        "forms": [
                            {
                                "type": "submit",
                                "id": "set-credentials",
                                "css": "btn-primary pull-right",
                                "text": "Set",
                                "attributes": [
                                    {
                                        "key": "data-form",
                                        "value": "login-credentials"
                                    },
                                    {
                                        "key": "data-field-username",
                                        "value": "device_username"
                                    },
                                    {
                                        "key": "data-field-password",
                                        "value": "device_password"
                                    },
                                    {
                                        "key": "data-field-repeat",
                                        "value": "device_password_repeat"
                                    }
                                ]
                            },
                            {
                                "css": "btn-danger pull-right",
                                "text": "Cancel",
                                "attributes": [
                                    {
                                        "key": "data-dismiss",
                                        "value": "modal"
                                    }
                                ]
                            }
                        ]
                    }
                },
                "close": "Cancel"
            },
            {
                "id": "backup",
                "title": "Backup Your Wallet",
                "body": "<p>By default, the information for this wallet is stored in LocalStorage, which resides within this browsers memory. If you copy and paste the information below into a plain text file you can import it again later if information or the device itself is lost. Alternatively, clicking on the save buttons below will try to export the information into a file for you. Please note that saving your wallet will try to back-up all accounts and contacts and can get fairly large quite quickly, whereas saving your salts will only save your primary account information and would force you to recreate the accounts and contacts again - so only do this if you remember the account details used previously. Future versions will include more practical ways to ensure all accouts can be saved without overloading buffers, but for now, please take care!</p>",
                "close": "Cancel",
                "form": {
                    "fields": [
                        {
                            "areas": {
                                "id": "data",
                                "css": "data-backup",
                                "style": "display: none;"
                            }
                        }
                    ],
                    "buttons": {
                        "forms": [
                            {
                                "type": "submit",
                                "id": "save-wallet",
                                "css": "btn-primary pull-right",
                                "text": "Save Wallet",
                                "attributes": [
                                    {
                                        "key": "data-form-id",
                                        "value": "backup"
                                    }
                                ]
                            },
                            {
                                "type": "submit",
                                "id": "save-salt",
                                "css": "btn-primary pull-right",
                                "text": "Save Salts Only",
                                "attributes": [
                                    {
                                        "key": "data-form-id",
                                        "value": "backup"
                                    }
                                ]
                            },
                            {
                                "id": "copy-salts",
                                "css": "btn-default pull-right btn-hidden_toggler",
                                "text": "Copy & Paste",
                                "attributes": [
                                    {
                                        "key": "data-id",
                                        "value": "data"
                                    }
                                ]
                            }
                        ]
                    }
                }
            },
            {
                "id": "send",
                "title": "Send Money",
                "body": "<p>Send money to one of your contacts in your wallet or enter a raw address.</p>",
                "close": "Cancel",
                "form": {
                    "css": "form-horizontal",
                    "fields": [
                        {
                            "inputs": {
                                "id": "to",
                                "label": {
                                    "text": "To",
                                    "css": "col-xs-3"
                                },
                                "type": "text",
                                "placeholder": "Type any address or click the user to select a contact",
                                "wrapper": {
                                    "css": "col-xs-9"
                                },
                                "icon": [
                                    {
                                        "css": "btn-toggle",
                                        "href": "#",
                                        "glyph": "user",
                                        "attributes": [
                                            {
                                                "key": "data-toggle",
                                                "value": "contacts"
                                            }
                                        ]
                                    }
                                ]
                            }
                        },
                        {
                            "selects": {
                                "id": "from",
                                "label": {
                                    "text": "From",
                                    "css": "col-xs-3"
                                },
                                "type": "text",
                                "css": "bs-account-select",
                                "placeholder": "Select Wallet",
                                "wrapper": {
                                    "css": "col-xs-9"
                                }
                            }
                        },
                        {
                            "inputs": {
                                "id": "fee",
                                "label": {
                                    "text": "Fee",
                                    "css": "col-xs-3"
                                },
                                "type": "text",
                                "placeholder": "Network Fee (Leave Blank for Default)",
                                "wrapper": {
                                    "css": "col-xs-9"
                                }
                            }
                        },
                        {
                            "inputs": {
                                "id": "amount",
                                "label": {
                                    "text": "Amount",
                                    "css": "col-xs-3"
                                },
                                "type": "text",
                                "placeholder": "Total to Send",
                                "wrapper": {
                                    "css": "col-xs-9"
                                }
                            }
                        }
                    ],
                    "buttons": {
                        "forms": [
                            {
                                "type": "submit",
                                "id": "send-money",
                                "css": "btn-primary pull-right",
                                "text": "Send",
                                "attributes": [
                                    {
                                        "key": "data-form-id",
                                        "value": "send"
                                    }
                                ]
                            }
                        ]
                    }
                }
            },
            {
                "id": "new-account",
                "title": "New Account",
                "body": "<p>An account can contain multiple addresses for multiple blockchains, and uses HD functionality by default, allowing you to generate multiple key pairs from a single seed. Alternatively, you could just pick the specific individual blockchain that you need.</p>",
                "close": "Cancel",
                "form": {
                    "css": "form-horizontal",
                    "fields": [
                        {
                            "selects": {
                                "id": "wallet_blockchain",
                                "css": "bs-blockchain-select hd prepend",
