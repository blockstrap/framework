                        },
                        {
                            "inputs": {
                                "id": "wallet_password_repeat",
                                "label": {
                                    "text": "Repeat",
                                    "css": "col-xs-3"
                                },
                                "placeholder": "Better to be safe than sorry",
                                "css": "ignore",
                                "type": "password",
                                "wrapper": {
                                    "css": "col-xs-9"
                                },
                                "attributes": [
                                    {
                                        "key": "data-repeat-id",
                                        "value": "wallet_password"
