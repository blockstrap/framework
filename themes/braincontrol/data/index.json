                                "attributes": [
                                    {
                                        "key": "data-form",
                                        "value": "new-account"
                                    },
                                    {
                                        "key": "data-before",
                                        "value": "extra_salty_wallet"
                                    },
                                    {
                                        "key": "data-setup-type",
                                        "value": "wallet"
                                    }
                                ],
                                "wrapper": {
                                    "css": "col-xs-9"
                                }
                            }
                        },
                        {
                            "inputs": {
                                "id": "wallet_question",
                                "label": {
                                    "text": "Custom Question",
                                    "css": "col-xs-3 hidden-label"
