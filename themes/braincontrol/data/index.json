                                "placeholder": "This should be unique to this individual wallet",
                                "type": "password",
                                "wrapper": {
                                    "css": "col-sm-9"
                                },
                                "css": "optional",
                                "attributes": [
                                    {
                                        "key": "data-setup-type",
                                        "value": "wallet"
                                    }
                                ]
                            }
                        },
                        {
                            "inputs": {
                                "id": "wallet_answer_repeat",
                                "label": {
                                    "text": "Repeat",
                                    "css": "col-sm-3 hidden-label"
